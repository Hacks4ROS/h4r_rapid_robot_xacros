<launch>
	<include file="$(find rapid_robot_xacros)/launch/empty_world.launch"/>
	<include file="$(find rapid_robot_xacros)/launch/test_bot_description.xml" />
	
  	<node pkg="robot_state_publisher" type="state_publisher" name="robot_state_publisher">
    	<param name="publish_frequency" type="double" value="30.0"/>
    	<param name="tf_prefix" type="string" value=""/>
  	</node>
    <node name="spawn_test_bot" pkg="gazebo_ros" type="spawn_model" 
          args="-z 0.1 -urdf -param robot_description -model robot_description"
    	  required="false" />
			<node pkg="rviz" type="rviz" name="rviz" />
</launch>